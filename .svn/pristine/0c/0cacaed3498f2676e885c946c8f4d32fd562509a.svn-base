	public static PayResource getInstance(Context context) {
		if (instance == null) {
			synchronized (PayResource.class) {
				if (instance == null) {
					instance = new PayResource(context);
				}
			}
		}
		return instance;
	}
	
	private PayResource(Context context) {
		mResourceUtil = ResourceUtil.getInstance(context);
		if(mHadInit){
			return;
		}else{
			init();
		}
	}
	
